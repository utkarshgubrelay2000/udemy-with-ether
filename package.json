  "scripts": {
    "dev-windows": "set NODE_ENV=development && next dev -p 8080",
    "dev": " next dev -p 8080",
    "build": "next build",
    "start": "next start -p 8000",
    "export": "next build && next export",
    "lint": "eslint --ignore-path .gitignore --ignore-path .eslintignore .",
    "deploy-ecr": "docker build -t workschool-web-service . && docker tag workschool-web-service:latest 513978991782.dkr.ecr.us-east-1.amazonaws.com/workschool-web-service:latest && docker push 513978991782.dkr.ecr.us-east-1.amazonaws.com/workschool-web-service:latest",
    "deploy-s3": "aws s3 cp --recursive ./out s3://www.workschool.co",
    "deploy-s3:beta": "aws s3 cp --recursive ./out s3://workschool-beta",
    "invalidate": "aws cloudfront create-invalidation --distribution-id E389PNNQONK7Q0 --paths \"/*\"",
    "danger:rmc": "docker rm $(docker ps -aq)",
    "danger:rmi": "docker rmi $(docker images -q)",
    "login-aws": "eval `aws ecr get-login --no-include-email --region us-east-1`",
    "deploy": "npm run deploy-s3 && npm run invalidate",
    "serve": "serve ./out -p 8080",
    "rm": "cd out/course/ && sh ../../remove_html_ext.sh"
  },
